{
  "Comment": "Details of tours completed",
  "name": "Mark Tour Complete",
  "version": "1.0",
  "categories": [
    "system"
  ],
  "StartAt": "SetTourData",
  "States": {
    "SetTourData": {
      "Type": "Task",
      "Resource": "module:setContextData",
      "ResourceConfig": {
        "$.tourData.completed": "$.completed",
        "$.tourData.userId": "$USERID"
      },
      "Next": "Upsert"
    },
    "Upsert": {
      "Type": "Task",
      "InputPath": "$.tourData",
      "Resource": "module:upserting",
      "ResourceConfig": {
        "modelId": "tourStatus"
      },
      "ResultPath": "$.tour",
      "End": true
    }
  },
  "restrictions": [
    {
      "roleId": "system_system",
      "allows": [
        "*"
      ]
    }
  ]
}
